int fun(object o) {
  print_k(o.x);
}

int main () {
  object a = {|
		string test_key : "Hello",
		int x : 123,
		object b : {| string hello : "World!" |}
	     |};

  object b = {|
		string test_key : "Hello...",
		object b : {| string hello : "Mars!", int x : 111 |},
		int x : 456,
		bool a : true
	     |};

  print_k(a.test_key);
  print_k(a.b.hello);
  print_k(a.x);

  print_k(b.test_key);
  print_k(b.b.hello);
  print_k(b.x);
  print_k(b.b.x);
  print_k(b.a);

  print_k(a.test_key);
  print_k(a.b.hello);
  print_k(a.x);
  print_k(a.z);
  print_k(a);

  fun(a);

}
