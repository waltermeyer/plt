// jsonify (from LRM)

string course = "PLT";
int students = 125;
string location = "NWC";
int [] my_arr = [1,2,3]
int x = 42;
int y = 2;
object course_obj = {
    string course : course;
    int students : students;
    string location : location;
    int [] my_arr : [4, 5, 6];
    int y : x + y;
    object my_stuff : {
        string location: course_obj.location;
        string location2: location;
        int [] my_arr = my_arr;
        int [] my_arr_2 = course_obj.my_arr;
    };
};

string course_str = jsonify(course_obj);
print(course_str);
// prints {course:"PLT", students:125, location:"NWC", my_arr:[1,2,3],
// y:44, my_stuff: {location:"NWC", location2:"NWC", my_arr:[1,2,3],
// my_arr_2:[4,5,6]}}
