int fun(object o) {
  print_k(o.x);
}

int main () {
  object a = {|
		string test_key : "Hello",
		int x : 123,
		object b : {| string hello : "World!" |}
	     |};

  object b = {|
		string test_key : "Hello...",
		object b : {| string hello : "Mars!", int x : 111 |},
		int x : 456,
		bool a : true
	     |};

  print_k(a.test_key);
  print_k(a.b.hello);
  print_k(b.x);
  print_k(b.a);

  // reassign keys to new values/types from primitives

  int i = 42;
  bool z = true;
  string s = "Meaning of life";
  float f = 97.1;

  a.test_key = i;
  a.b.hello = z;
  b.x = s;
  b.a = f;
  a.x = 42.42;

  print_k(a.test_key);
  print_k(a.b.hello);
  print_k(b.x);
  print_k(b.a);
  print_k(a.x);
  a.x = b.x;
  print_k(a.x);

}
